programa {
  inteiro numero1, numero2
  funcao inicio() {
    escreva("Digite um número: ")
    leia(numero1)
    escreva("Digite o segundo número: ")
    leia(numero2)


    escreva("a soma dos números é igual à ", Somar(numero1, numero2))
  }
  funcao inteiro Somar(inteiro n1, inteiro n2){
    retorne n1 + n2

  }
}