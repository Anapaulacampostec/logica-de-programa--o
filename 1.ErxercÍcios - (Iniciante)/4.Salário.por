programa {
  funcao inicio() {
    real salHora, horaTrabalhada, total

    escreva("Digite da hora trabalhada: R$ ")
    leia(salHora)
    escreva("Digite a quantidade de horas trabalhadas no mês: ")
    leia(horaTrabalhada)

    total = salHora * horaTrabalhada

    escreva("Seu slário é de R$ ", total)

  }
}
